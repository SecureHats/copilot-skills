{
  "if": {
    "allOf": [
      {
      "field": "type",
      "equals": "Microsoft.SecurityCopilot/capacities"
      },
      {
      "field": "Microsoft.SecurityCopilot/capacities/numberOfUnits",
      "greater": "parameters('numberOfUnits')"
      }
    ]
    },
    "then": {
      "effect": "[parameters('effectType')]"
    }
  }
}
